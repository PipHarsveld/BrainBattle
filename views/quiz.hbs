<header>
    {{> header}}
</header>
<main class="quiz">
    <p>Here's your first question:</p>
    <p>{{ question }}</p>
    <section>
        {{#each answers}}
            <button value="{{ this }}">{{ this }}</button>
        {{/each}}
    </section>

    <a href="/quiz" class="button">New question</a>
</main>